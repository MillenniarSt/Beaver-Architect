{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "project",
  "description": "Capability for the project window",
  "windows": ["project"],
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview-window",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-close",
    "opener:default",
    "dialog:default",
    "fs:default",
    "shell:default",
    {
      "identifier": "fs:scope",
      "allow": ["**/*"]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "run-server",
          "cmd": "C:\\Users\\Ange\\AppData\\Roaming\\io.github.MillenniarSt.Beaver-Architect\\server.exe",
          "args": true
        },
        {
          "name": "run-minecraft-architect",
          "cmd": "C:\\Users\\Ange\\AppData\\Roaming\\io.github.MillenniarSt.Beaver-Architect\\architects\\minecraft\\architect.exe",
          "args": true
        }
      ]
    }
  ]
}
