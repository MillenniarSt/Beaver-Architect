{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview-window",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:write-files",
    {
      "identifier": "fs:scope",
      "allow": [
        "**/*"
      ]
    }
  ]
}